{
  "name": "assistant",
  "version": "1.5.0",
  "description": "A voice assistant",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node --es-module-specifier-resolution=node ./dist/index.js",
    "debug": "node --es-module-specifier-resolution=node ./dist/index.js --no-cache --no-cluster --debug",
    "build": "npm run clean && npx tsc && npx tsc-alias",
    "watch": "npm run build && tsc-watch --onSuccess \"npm run on-recompile\"",
    "plugin": "node ./scripts/create-plugin.js",
    "pretty": "npx prettier --write .",
    "clean": "node ./scripts/clean.js",
    "on-recompile": "npx tsc-alias",
    "copy-plugin-assets": "node ./scripts/copy-plugin-assets.js"
  },
  "keywords": [],
  "author": "Oyintare",
  "license": "ISC",
  "dependencies": {
    "@types/node-notifier": "^8.0.2",
    "axios": "^0.24.0",
    "chokidar": "^3.5.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "ffmpeg-static": "^4.4.0",
    "heatsync": "^2.2.1",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "node-notifier": "^10.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/node": "^7.15.4",
    "@types/express": "^4.17.16",
    "@types/node": "^17.0.34",
    "@typescript-eslint/eslint-plugin": "^5.24.0",
    "@typescript-eslint/parser": "^5.24.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-promise": "^6.0.0",
    "prettier": "2.7.1",
    "tsc-alias": "^1.8.2",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.7.4"
  }
}
